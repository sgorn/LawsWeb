            <asp:TextBox CssClass="wordSize" ID="txtS3arch0r3" runat="server" title="Word 3"></asp:TextBox> 
        </fieldset>       
        </div>
        <div class="keyword">
            <label for="txtS3archN0t" class="searchLabel">none of these words:</label>
            <asp:TextBox ID="txtS3archN0t" runat="server"></asp:TextBox>        
        </div>
         <div class="keyword">
         <fieldset class="advancedSearch">
         <legend><span>Word proximity:</span></legend>
            <asp:TextBox ID="txtPr0x1" CssClass="wordSize" runat="server" title="Proximity word 1"></asp:TextBox> 
within  
            <select id="ddPr0x" runat="server" title="Word proximity">
            <option>1</option>
            <option>2</option>
            <option>3</option>
            <option>4</option>
            <option selected="selected">5</option>
            <option>6</option>
            <option>7</option>
            <option>8</option>
            <option>9</option>
            <option>10</option>
            </select>
             words of 
            <asp:TextBox ID="txtPr0x2" CssClass="wordSize" runat="server" title="Proximity word 2"></asp:TextBox>    
        </fieldset> 
        </div>
        <div class="keyword">
            <label for="txtT1tl3" class="searchLabel">Title:</label>
            <asp:TextBox ID="txtT1tl3" runat="server"></asp:TextBox>        
        </div>
