        <p class="searchIntro">Show results that have...</p>
            <div class="keyword">
            <label for="txtS3archA11" class="searchLabel">all these words:</label>
            <asp:TextBox ID="txtS3archA11" runat="server"></asp:TextBox>        
        </div>
        <div class="keyword">
            <label for="txtS3arch3xact" class="searchLabel">this exact phrase:</label>
            <asp:TextBox ID="txtS3arch3xact" runat="server"></asp:TextBox>        
        </div>
        <div class="keyword">
        <fieldset class="advancedSearch">
            <legend><span>one or more of these words:</span></legend>
            <asp:TextBox CssClass="wordSize" ID="txtS3arch0r1" runat="server" title="Word 1"></asp:TextBox> 
